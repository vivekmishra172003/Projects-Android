<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Add any required permissions here -->
    <uses-permission android:name="android.permission.INTERNET"/> <!-- Example permission if needed -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hostelf"
        tools:targetApi="31">

        <activity
            android:name="com.example.hostelf.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Add any additional activities here -->
        <activity android:name="com.example.hostelf.RemainingRoomsActivity" android:exported="false" />
    </application>

</manifest>
